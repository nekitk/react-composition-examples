{
  "presets": [
    "env"
  ],
  "plugins": [
    [
      "transform-react-jsx",
      {
        "pragma": "React.createElement"
      }
    ],
    "transform-class-properties",
    "transform-object-rest-spread",
    "transform-decorators-legacy",
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }]
  ]
}